<div class="w3-display-container" style="height:300px;display:none;" id="loginBox">
  <div class="w3-middle">
    <div class="w3-container w3-light-blue">
      <h2>Log in</h2>
    </div>

    <div class="w3-container" id="inputForm" onkeypress="onLoginInput(event)">
      <label>Tester ID</label>
      <input class="w3-input" type="text" name="testerID" autofocus>
      <label>Password</label>
      <input class="w3-input" type="password" name="password">
    </div>
      <button id="loginButton" class="w3-round w3-btn-block w3-blue w3-ripple w3-hover-indigo" onclick="loginRequest()" >Log in </button>
  </div>
</div>
