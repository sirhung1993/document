    <div class="w3-row">
        <div class="w3-col " style="width:30%"><p></p></div>
        <div class="w3-panel w3-half w3-row w3-border" style="width:40%">  
            <p> </p>
            <p>
            <div style="position: relative;">
                <input placeholder="File to up load"  class="w3-text-black w3-btn w3-white w3-left-align w3-border" style="width:100%;" 
                id="UploadFile" type="text"></input> 
                <input value="Browse" type="button" class="w3-btn w3-light-blue w3-hover-blue"
                style="position:absolute;right:0px;"
                onclick="BrowseFile()">
                </input>
            </div>
            </p>
            <p>
            <form action="/upload/picture" method="post" enctype="multipart/form-data" target="myiframe" id="form" >
                <input class="w3-block" type="file" value="Browse" 
                style="display:none" id="FileBrowser" name="uploadFileName">
                <input class="w3-btn w3-light-blue w3-hover-blue " type="submit" value="Upload" id="submit" onclick="$('#uploadOk').show()"/>
            </form>
            <iframe name="myiframe" style="display:none">
            </iframe>    
            </p>
        </div>
        <div class="w3-modal w3-animate-zoom" id="uploadOk" style="display:none" style="width:40%">
                
            <div class="w3-modal-content" >
                <div class="w3-display w3-green">              
                    <span onclick="document.getElementById('uploadOk').style.display='none';
                        location.reload(true);" class="w3-display-topright w3-btn w3-light-green w3-hover-green" style="cursor:pointer">&times; </span>
                </div>
                <div class="w3-green">
                    <br/>
                    <div class="w3-center w3-block w3-xlarge">
                        File upload successfully!</div>         
                    <br/>                
                </div>
            </div>
        </div>
    </div>
    <div class="w3-row w3-animate-opacity" id="CopyOk" style="display:none;top:10px">
